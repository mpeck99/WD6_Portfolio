{{# each products}}
    products
{{/each}}
<h1>Our Products</h1>
<div class="row">
{{# each products }}
                <div class="col-sm-12 col-md-6 col-lg-4">
                    <div class="card product">
                        <img class="card-img-top image-responsive" src={{ this.imagePath }} alt="Shoe">
                        <div class="card-body">
                            <h5 class="card-title">{{ this.title }}</h5>
                            <p class="card-text product-info">{{ this.description }}</p>
                            <div>
                                <div class="price float-left">${{ this.price }}</div>
                                <a href="#" class="btn btn-primary float-right addCart">Add to Cart</a>
                            </div>

                        </div>
                    </div>
                </div>
{{/each}}
</div>